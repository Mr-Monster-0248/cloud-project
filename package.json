{
  "name": "rest-project",
  "version": "0.0.1",
  "description": "node rest project",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cvbenur/node-rest-project.git"
  },
  "keywords": [
    "node",
    "rest",
    "API"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cvbenur/node-rest-project/issues"
  },
  "homepage": "https://github.com/cvbenur/node-rest-project#readme",
  "scripts": {
    "test": "echo not implemented",
    "build": "tsc",
    "start": "node .",
    "dev": "nodemon src/index.ts",
    "migration:run": "tsc && ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/",
      "dist/"
    ],
    "watch": [
      "./src"
    ],
    "execMap": {
      "ts": "node -r ts-node/register"
    },
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.23.4",
    "dotenv": "^11.0.0",
    "fastify": "^3.25.3",
    "fastify-plugin": "^3.0.0",
    "fastify-swagger": "^4.15.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.44"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.3.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.4"
  }
}
